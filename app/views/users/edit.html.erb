<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name_family, autofocus: true, id:"inputName", placeholder:"姓"%>
  <%= f.text_field :name_first, autofocus: true, id:"inputName", placeholder:"名"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.submit "変更を保存" %>
<% end %>